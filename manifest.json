{
    "name": "DarkPG",
    "version": "1.2",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["https://playground.digitalhouse.com/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "browser_action": {
      "default_title": "DarkPG"
    },

    "permissions": ["tabs"]
  }

  